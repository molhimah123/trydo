{
  "name": "trydo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:status": "supabase migration list",
    "db:diff": "supabase db diff --schema public",
    "db:migrate:staging": "supabase db push --project-ref $STAGING_PROJECT_REF",
    "db:migrate:production": "supabase db push --project-ref $PRODUCTION_PROJECT_REF",
    "db:verify:staging": "supabase db diff --project-ref $STAGING_PROJECT_REF",
    "db:verify:production": "supabase db diff --project-ref $PRODUCTION_PROJECT_REF",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "check-cicd": "bash scripts/check-cicd.sh"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.45.4",
    "next": "^15.5.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
